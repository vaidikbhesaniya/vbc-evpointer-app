{
    "expo": {
        "name": "evpointer-vbc",
        "slug": "evpointer-vbc",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "platforms": ["android", "ios"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.vaidik1212.evpointervbc"
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/images/favicon.png"
        },
        "plugins": ["expo-router"],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "93e3cec1-200e-4756-aaae-8f3d85aa0e56"
            }
        }
    }
}
